<Window x:Class="DomainHomeWork.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DomainHomeWork"
        mc:Ignorable="d"
        Title="MainWindow" Height="250" Width="200">
    <Grid>
        <StackPanel>
            <Button Name="download" Content="Скачать" Height="40" Width="100" Margin="30" Click="Download"></Button>
            <Button Name="operation" Content="Запустить" Height="40" Width="100" Click="Operation"></Button>
        </StackPanel>
    </Grid>
</Window>
